{
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "testRegex": "/test/.*\\.spec\\.ts$",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{ts,tsx}",
    "!**/node_modules/**"
  ],
  "coverageReporters": [
    "lcovonly",
    "text"
  ]
}
