{
	"compilerOptions": {
		"declaration": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"jsx": "react",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"inlineSourceMap": true,
		"lib": [
			"es2017"
		]
	},
	"include": [
		"src/index.ts"
	]
}
