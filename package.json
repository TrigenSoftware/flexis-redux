{
  "name": "@flexis/redux",
  "version": "0.1.3",
  "description": "Wrapper for Redux.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/flexis-redux.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-redux/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "scripts": {
    "lint": "eslint --cache 'src/**/*.js' 'test/**/*.js'",
    "test": "npm run lint && NODE_ENV=test jest",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "immutable"
  ],
  "dependencies": {
    "@types/react": "^16.3.16",
    "hoist-non-react-statics": "^2.5.0",
    "immutable": "4.0.0-rc.9"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "^3.0.0",
    "escope": "^3.6.0",
    "eslint": "^4.10.0",
    "eslint-config-trigen": "^2.4.0",
    "eslint-plugin-jest": "^21.15.0",
    "jest": "^22.4.3",
    "redux": "^4.0.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.5",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "redux": "^4.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions",
              "iOS > 7",
              "Android > 4.4"
            ]
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "/test/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcovonly",
      "text"
    ]
  },
  "files": [
    "lib"
  ]
}
