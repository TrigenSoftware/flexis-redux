{
  "name": "@flexis/redux",
  "version": "1.0.0-alpha",
  "description": "Wrapper for Redux.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/flexis-redux.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/flexis-redux/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "generateDocs": "typedoc ./src --out ./docs --ignoreCompilerErrors --excludeExternals --mode modules; touch docs/.nojekyll",
    "checkDeps": "npm-check -s; exit 0",
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "test": "npm run checkDeps && npm run lint && npm run jest",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "immutable"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@types/node": "^10.9.2",
    "@types/prop-types": "^15.5.5",
    "@types/react": "16.4.12",
    "hoist-non-react-statics": "^3.0.1",
    "immutable": "4.0.0-rc.9"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/jest": "^23.3.1",
    "babel-preset-trigen": "^1.3.0",
    "browserslist-config-trigen": "^1.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "jest": "^23.5.0",
    "npm-check": "^5.8.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.2",
    "redux": "^4.0.0",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-tslint": "^0.1.35",
    "rollup-plugin-typescript2": "^0.17.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-trigen": "^3.0.6",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  },
  "files": [
    "lib"
  ]
}
